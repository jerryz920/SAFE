#!/bin/bash

if [ -z "$SAFE_ADDR" ]; then
    echo ' must set SAFE_ADDR env '
    exit 1
fi


postInstanceSet() {
    local principal=$1
    shift 1
# ?Instance,?Image,?AuthID,?ImageStoreOwner
    curl -XPOST $SAFE_ADDR/postInstanceSet -d "{ \"principal\": \"$principal\", \"otherValues\": [\"${1}\",\"${2}\",\"${3}\",\"${4}\"]}"
}
delInstanceSet() {
    local principal=$1
    shift 1
    curl -XPOST $SAFE_ADDR/delInstanceSet -d "{ \"principal\": \"$principal\", \"otherValues\": [\"${1}\",\"${2}\",\"${3}\",\"${4}\"]}"
}
        

postVMInstanceSet() {
    local principal=$1
    shift 1
# ?Instance,?Image,?AuthID,?Cidr,?ImageStoreOwner,?Vpc
    curl -XPOST $SAFE_ADDR/postVMInstanceSet -d "{ \"principal\": \"$principal\", \"otherValues\": [\"${1}\",\"${2}\",\"${3}\",\"${4}\",\"${5}\",\"${6}\"]}"
}
delVMInstanceSet() {
    local principal=$1
    shift 1
    curl -XPOST $SAFE_ADDR/delVMInstanceSet -d "{ \"principal\": \"$principal\", \"otherValues\": [\"${1}\",\"${2}\",\"${3}\",\"${4}\",\"${5}\",\"${6}\"]}"
}
        

